                    <view class="content">
                        <view class="name">{{item.title}}</view>
                        <view class="level">{{item.level}}</view>
                        <view class="line"></view>
                    </view>
                    <view class="header_img">
                        <image mode="widthFix" src="{{baseUrl + item.tximg}}" />
