<view class="photographer">
    <view class="left_menu">
        <scroll-view scroll-y="true" style="height: {{winHeight+'px'}}">
            <view class="menu_item {{isyf ? 'active':''}}" bind:tap="yfTap">研发中心</view>
            <view class="menu_item {{currentTab==index ? 'active' :'' }}" data-current='{{index}}' wx:for="{{storeList}}" wx:key="id" data-id="{{item.id}}" bindtap='switchNav'>
                {{item.name}}
            </view>
        </scroll-view>
