/* pages/studio/order.wxss */
.customer_service{position:fixed;margin:0;padding:0;width:80rpx;height:80rpx;border-radius:40rpx;box-shadow:0px 10px 30px 0px rgba(0,0,0,0.15);right:40rpx;top:72.7%;z-index:10;}

.customer_service:after{border:none;}
.personal{padding:20rpx 36rpx 0 36rpx;overflow:hidden;}
.page_bj{height:100%; overflow:hidden}

.dingdanh .ddh{color:#888888;}
.list{height:200rpx;display:flex;justify-content:space-between;flex-direction:row;align-items:center;background:#fff;margin-bottom:20rpx;background:#fff;position:relative;-webkit-transition:margin-left .3s ease-in-out;-moz-transition:margin-left .3s ease-in-out;transition:margin-left .3s ease-in-out;}


.list .tpimg{width:132rpx;height:176rpx;text-align:center;margin-left:12rpx;margin-right:28rpx;}

.list .dj_jg{width:350rpx;height:176rpx;}
.list .dj_jg .biaoti{height:50rpx;line-height:50rpx;}
.list .dj_jg text{margin-right:20rpx;}
.list .dj_jg .dzf{color:#E47A0B;font-weight:bold;}
.list .dj_jg .yzf{color:#888888;font-weight:bold;height:40rpx;line-height:40rpx;}
.list .dj_jg .time{color:#D8D8D8;margin-top:50rpx;height:40rpx;line-height:40rpx;}

.list .an{width:210rpx;margin-right:20rpx;}
.list .an navigator{width:140rpx;height:60rpx;display:block;text-align:center;line-height:60rpx;color:#fff;font-weight:bold;margin:0 auto;}
.list .an .payment{background:#B1825F;}
.list .an .see{border:1rpx solid #888888;color:#888888;}
.list .an .time{color:#9B9B9B;margin:0 auto;margin-top:6rpx;}

.list .delete{position:absolute;right:-115rpx;width:80rpx;height:200rpx;z-index:5;margin-left:0;}
.list.active{margin-left:-110rpx}

.nodata{width:100%;height:100vh;display:flex;align-items:center;justify-content:center;}
.nodata .img{width:648rpx;height:690rpx;}